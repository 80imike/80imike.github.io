// 标题，修改成你期望的字体族
$font-family-headings = Georgia, sans, 'PT Serif', 'STZhongsong', '华文中宋', 'Microsoft Yahei', serif, Verdana, sans-serif

// 修改成你期望的字体族
$font-family-base = 'PT Serif', 'STZhongsong', '华文中宋', 'Microsoft Yahei', serif, Verdana, sans-serif

// 代码字体
$code-font-family = "Source Code Pro", Consolas, Monaco, Menlo, monospace, '华文中宋', "Input Mono", "PT Mono"

// 正文字体的大小
$font-size-base = 16px

// 代码字体的大小
$code-font-size = 13px